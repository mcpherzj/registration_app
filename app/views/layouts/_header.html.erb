<header>
	<%= link_to logo, root_path %>
	<nav class="round">
		<ul id="menu">
			<li><%= link_to "Home", root_path %></li>
			<% if signed_in? %>
			<li><%= link_to "Registrations", registrations_path %></li>
			<% end %>
			<li><%= link_to "Help", '#' %></li>
            <% if signed_in? %>
            <li><%= link_to "Sign out", signout_path, :method => :delete %></li>
            <% else %>
            <li><%= link_to "Sign in", signin_path %></li>
            <% end %>
		</ul>
	</nav>
</header>
